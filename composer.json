                },
                "require": {
                    "philippbaschke/acf-pro-installer": "^1.0",
                    "composer/installers": "^1.0"
                }
            }
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "composer/installers": "~1.0",
        "roots/soil" : ">=3.7.1",
        "roots/wp-h5bp-htaccess" : ">=2.0.1",
        "jjgrainger/posttypes": "dev-master",
        "advanced-custom-fields/advanced-custom-fields-pro": "*"
    },
    "extra": {
        "installer-paths": {
            "vendor/{$name}": ["vendor/package"]
